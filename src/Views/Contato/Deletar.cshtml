@model ContatoModel
@{
    ViewData["Title"] = "Deletar contato";
}
<div class="text-center">
    <h3>Você deseja remover o contato(@Model.Nome)?</h3>
    <br />
</div>

<a role="button"class="btn-primary btn-primary btn d-block" asp-action="Index" asp-controller="Contato">Cancelar exclusão </a>
</br>
<a role="button" class="btn btn-danger btn d-block" asp-action="DeletarConfirmacao" asp-controller="Contato" asp-route-id="@Model.Id">Confirmar exclusão</a>



